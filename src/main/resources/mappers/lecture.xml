<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.web.study.respository.LectureRepository">
  
  <insert id="registe" parameterType="com.web.study.domain.entity.Lecture">
  	insert into lecture_mst
  	values(0,#{lecture_name}, #{lecture_price}, #{lecturer_id})
  </insert>
  
  <select id="getLectureAll" resultType="com.web.study.domain.entity.Lecture">
	  select
	  	id,
	  	lecture_name,
	  	lecture_price,
	  	lecturer_id
	  from
	  	lecture_mst
  </select>
  
  <select id="findlectureById" resultType="com.web.study.domain.entity.Lecture">
	  select
	  	id,
	  	lecture_name,
	  	lecture_price,
	  	lecturer_id
	  from
	  	lecture_mst
	  where
	  	id = #{id}
  </select>
  
</mapper>